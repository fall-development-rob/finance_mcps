{
  "name": "@corp-finance/bindings",
  "version": "0.1.0",
  "description": "NAPI-RS bindings for corp-finance-core Rust library",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "corp-finance-bindings",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release --cargo-cwd ../../",
    "build:debug": "napi build --platform --cargo-cwd ../../",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "version": "napi version"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "dependencies": {
    "@napi-rs/wasm-runtime": "^0.2.0"
  },
  "engines": {
    "node": ">= 18"
  }
}
